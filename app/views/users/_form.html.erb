<%= form_with model: @user do |f|%>
    <%= f.label :uid , "User ID : "%>
    <%= f.text_field :uid , placeholder:"Enter User ID"%><br><br>

    <%= f.label :name , "Name : "%>
    <%= f.text_field :name , placeholder:"Enter User Name"%><br><br>

    <%= f.label :age , "Age : "%>
    <%= f.text_field :age , placeholder:"Enter User Age"%><br><br>

    <%= f.label :email , "Email : "%>
    <%= f.text_field :email , placeholder:"Enter User Email"%><br><br>

    <%= f.label :address , "Address : "%>
    <%= f.text_field :address , placeholder:"Enter User Address"%><br><br>

    <%= f.submit "Save User"%>
<% end %>